<?xml version="1.0" encoding="UTF-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
    <file original="" product-name="CodeQ.Revisions" source-language="en" target-language="de" datatype="plaintext">
        <body>
            <trans-unit id="header.label">
                <source>Label</source>
                <target>Bezeichnung</target>
            </trans-unit>
            <trans-unit id="header.actions">
                <source>Actions</source>
                <target>Aktionen</target>
            </trans-unit>
            <trans-unit id="header.identifier">
                <source>Identifier</source>
                <target>Kennung</target>
            </trans-unit>
            <trans-unit id="header.creator">
                <source>Creator</source>
                <target>creator</target>
            </trans-unit>
            <trans-unit id="header.revision">
                <source>Revision</source>
                <target>Revision</target>
            </trans-unit>

            <trans-unit id="list.noRevisionsFound">
                <source>No revisions found</source>
                <target>Keine Revisionen gefunden</target>
            </trans-unit>

            <trans-unit id="revision.label">
                <source>By {creator}</source>
                <target>Von {creator}</target>
            </trans-unit>
            <trans-unit id="tooltip.revisionLabel">
                <source>Created on {revisionDate} by {creator}</source>
                <target>Erstellt am {revisionDate} von {creator}</target>
            </trans-unit>

            <trans-unit id="action.close">
                <source>Close</source>
                <target>Schließen</target>
            </trans-unit>
            <trans-unit id="action.apply">
                <source>Apply</source>
                <target>Anwenden</target>
            </trans-unit>
            <trans-unit id="action.apply.newRevisionLabel">
                <source>Applied revision {revision}</source>
                <target>Angewandte Revision {revision}</target>
            </trans-unit>

            <trans-unit id="action.apply.title">
                <source>Apply revision {revisionDate} by {creator}</source>
                <target>Revision {revisionDate} von {creator} anwenden</target>
            </trans-unit>
            <trans-unit id="action.edit.title">
                <source>Edit revision {revisionDate} by {creator}</source>
                <target>Revision {revisionDate} von {creator} bearbeiten</target>
            </trans-unit>
            <trans-unit id="action.delete.title">
                <source>Delete revision {revisionDate} by {creator}</source>
                <target>Revision {revisionDate} von {creator} löschen</target>
            </trans-unit>

            <trans-unit id="success.revisionApplied">
                <source>Revision applied</source>
                <target>Revision angewandt</target>
            </trans-unit>
            <trans-unit id="success.revisionApplied.message">
                <source>Revision "{label}" by "{creator}" applied.</source>
                <target>Revision "{label}" von "{creator}" angewendet.</target>
            </trans-unit>
            <trans-unit id="success.revisionDeleted">
                <source>Revision deleted</source>
                <target>Revision gelöscht</target>
            </trans-unit>
            <trans-unit id="success.revisionDeleted.message">
                <source>Revision "{label}" by "{creator} deleted.</source>
                <target>Revision "{label}" von "{creator} gelöscht.</target>
            </trans-unit>
            <trans-unit id="success.revisionUpdated">
                <source>Revision updated</source>
                <target>Revision aktualisiert</target>
            </trans-unit>
            <trans-unit id="success.revisionUpdated.message">
                <source>Revision {label} by "{creator}" updated.</source>
                <target>Revision {label} von "{creator}" aktualisiert.</target>
            </trans-unit>

            <trans-unit id="error.failedFetchingRevisions">
                <source>An error occurred while fetching revisions.</source>
                <target>Beim Abrufen von Revisionen ist ein Fehler aufgetreten.</target>
            </trans-unit>
            <trans-unit id="error.failedApplyingRevision">
                <source>An error occurred while applying revision.</source>
                <target>Bei der Anwendung der Revision ist ein Fehler aufgetreten.</target>
            </trans-unit>
            <trans-unit id="error.failedUpdatingRevision">
                <source>An error occurred while updating revision.</source>
                <target>Beim Aktualisieren der Revision ist ein Fehler aufgetreten.</target>
            </trans-unit>
            <trans-unit id="error.failedDeletingRevision">
                <source>An error occurred while deleting a revision.</source>
                <target>Beim Löschen einer Revision ist ein Fehler aufgetreten.</target>
            </trans-unit>
            <trans-unit id="error.verifyResolveConflicts">
                <source>Some conflicts were detected. Do you still want to apply the revision?{conflicts}</source>
                <target>Es wurden einige Konflikte festgestellt. Möchten Sie die Revision trotzdem anwenden?{conflicts}</target>
            </trans-unit>
            <trans-unit id="error.revisionNotFound">
                <source>Revision not found</source>
                <target>Revision nicht gefunden</target>
            </trans-unit>
            <trans-unit id="error.revisionHasConflicts">
                <source>Revision has conflicts</source>
                <target>Revision hat Konflikte</target>
            </trans-unit>
            <trans-unit id="error.nodeNotFound">
                <source>Page not found</source>
                <target>Seite nicht gefunden</target>
            </trans-unit>
            <trans-unit id="error.revisionNotApplied">
                <source>Failed to apply revision</source>
                <target>Revision konnte nicht angewandt werden</target>
            </trans-unit>

            <trans-unit id="confirm.applyRevision">
                <source>Do you want to apply revision {revisionDate} by {creator}?</source>
                <target>Möchten Sie Revision {revisionDate} von {creator} anwenden?</target>
            </trans-unit>

            <trans-unit id="diff.header">
                <source>Version "{version}" (created on {date}) contains the following changes:</source>
                <target>Version "{version}" (erstellt am {date}) enthält die folgenden Änderungen:</target>
            </trans-unit>
            <trans-unit id="diff.propertyLabel">
                <source>Modification of the "{propertyLabel}" property</source>
                <target>Änderung der Eigenschaft "{propertyLabel}"</target>
            </trans-unit>
            <trans-unit id="diff.removeNode">
                <source>Remove</source>
                <target>Entferne</target>
            </trans-unit>
            <trans-unit id="diff.addNode">
                <source>Add</source>
                <target>Füge hinzu</target>
            </trans-unit>
            <trans-unit id="diff.changeNode">
                <source>Update</source>
                <target>Aktualisiere</target>
            </trans-unit>
            <trans-unit id="diff.old">
                <source>Old</source>
                <target>Alt</target>
            </trans-unit>
            <trans-unit id="diff.new">
                <source>New</source>
                <target>Neu</target>
            </trans-unit>
        </body>
    </file>
</xliff>
